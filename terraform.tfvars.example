app_client_name            = "todotask_app_client"
api_name                   = "todotask_api"
file_names = {
  create-task    = "create-task.py"
  read-all-tasks = "read-all-tasks.py"
  update-task    = "update-task.py"
  delete-task    = "delete-task.py"
}
lambda_functions = {
  get = {
    handler  = "read-all-tasks.lambda_handler"
