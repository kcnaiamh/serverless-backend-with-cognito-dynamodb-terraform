    handler  = "create-task.lambda_handler"
    runtime  = "python3.9"
    filename = "./modules/lambda/zips/create-task.zip"
    env_vars = {}
  }
  patch = {
    handler  = "update-task.lambda_handler"
    runtime  = "python3.9"
    filename = "./modules/lambda/zips/update-task.zip"
    env_vars = {}
  }
  delete = {
    handler  = "delete-task.lambda_handler"
    runtime  = "python3.9"
    filename = "./modules/lambda/zips/delete-task.zip"
    env_vars = {}
  }
}
